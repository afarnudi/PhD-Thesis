	journal = {ACM Trans. Math. Softw.},
	keywords = {sphere, Dirichlet tessellation, Thiessen regions, Delaunay triangulation, Voronoi diagram},
	month = sep,
	number = {3},
	numpages = {19},
	pages = {416--434},
	publisher = {Association for Computing Machinery},
	title = {Algorithm 772: STRIPACK: Delaunay Triangulation and Voronoi Diagram on the Surface of a Sphere},
	url = {https://doi.org/10.1145/275323.275329},
	volume = {23},
