	journal = {Biophysical Journal},
	number = {5},
	pages = {2369-2381},
	title = {Influence of network topology on the elasticity of the red blood cell membrane skeleton},
	url = {https://www.sciencedirect.com/science/article/pii/S0006349597788829},
	volume = {72},
	year = {1997},
