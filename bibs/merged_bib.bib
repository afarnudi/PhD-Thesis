@article{peterson1985,
	abstract = { Abstract This paper considers the hydrodynamics of low
		  Reynolds number shape fluctuations of incompressible fluid
		  cells with incompressible membrane boundaries. It is
		  assumed that the membrane can be characterized by a shear
		  modulus μ, a bending modulus kc , an intrinsic mean
