	issn = {0036-8075},
	journal = {Science},
	number = {5950},
	pages = {289--293},
	publisher = {American Association for the Advancement of Science},
	title = {Comprehensive Mapping of Long-Range Interactions Reveals Folding Principles of the Human Genome},
