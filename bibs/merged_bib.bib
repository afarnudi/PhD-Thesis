	journal = {Nature Communications},
	number = {1},
	pages = {3161},
	title = {Interphase human chromosome exhibits out of equilibrium glassy dynamics},
	ty = {JOUR},
	url = {https://doi.org/10.1038/s41467-018-05606-6},
	volume = {9},
	year = {2018},
