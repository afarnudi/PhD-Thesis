	keywords = {Self-assembly; Molecular dynamics simulations; Bio-membranes; Coarse-graining; Structural properties; Phase diagram},
	number = {9},
	pages = {1955--1963},
	title = {On the dynamics of molecular self-assembly and the structural analysis of bilayer membranes using coarse-grained molecular dynamics simulations},
	url = {https://www.sciencedirect.com/science/article/pii/S0005273616301882},
	volume = {1858},
