@article{nuebler196261,
	abstract = {Mammalian chromatin is organized on length scales ranging
		  from individual nucleosomes to chromosomal territories. At
		  intermediate scales two dominant features emerge in
		  interphase: (i) alternating regions (\&lt;5Mb) of active
		  and inactive chromatin that spatially segregate into
		  different compartments, and (ii) domains (\&lt;1Mb), i.e.
		  regions that preferentially interact internally, which are
		  also termed topologically associating domains (TADs) and
		  are central to gene regulation. There is growing evidence
		  that TADs are formed by active extrusion of chromatin loops
		  by cohesin, whereas compartments are established by a phase
		  separation process according to local chromatin states.
		  Here we use polymer simulations to examine how the two
		  processes, loop extrusion and compartmental segregation,
		  work collectively and potentially interfere in shaping
		  global chromosome organization. Our integrated model
		  faithfully reproduces Hi-C data from previously puzzling
		  experimental observations, where targeting of the
		  TAD-forming machinery led to changes in
		  compartmentalization. Specifically, depletion of
		  chromatin-associated cohesin reduced TADs and revealed
		  hidden, finer compartments, while increased processivity of
		  cohesin led to stronger TADs and reduced
		  compartmentalization, and depletion of the TAD boundary
		  protein, CTCF, weakened TADs while leaving compartments
		  unaffected. We reveal that these experimental perturbations
		  are special cases of a general polymer phenomenon of active
		  mixing by loop extrusion. This also predicts that
		  interference with chromatin epigenetic states or nuclear
		  volume would affect compartments but not TADs. Our results
		  suggest that chromatin organization on the megabase scale
		  emerges from competition of non-equilibrium active loop
		  extrusion and epigenetically defined compartment structure.},
	author = {Nuebler, Johannes and Fudenberg, Geoffrey and Imakaev, Maxim and Abdennur, Nezar and Mirny, Leonid},
	date-added = {2018-10-16 17:12:50 +0000},
	date-modified = {2018-10-16 17:12:50 +0000},
	doi = {10.1101/196261},
	eprint = {https://www.biorxiv.org/content/early/2017/10/03/196261.full.pdf},
	journal = {bioRxiv},
	publisher = {Cold Spring Harbor Laboratory},
	title = {Chromatin Organization by an Interplay of Loop Extrusion and Compartmental Segregation},
	url = {https://www.biorxiv.org/content/early/2017/10/03/196261},
	year = {2017},
	bdsk-url-1 = {https://www.biorxiv.org/content/early/2017/10/03/196261},
	bdsk-url-2 = {https://doi.org/10.1101/196261}}

@article{openmm2017,
