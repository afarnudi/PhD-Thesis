@article{engelman:2005aa,
	abstract = {The wealth of new data on membrane protein structures and
		  functions is changing our general view of membrane
		  architecture. Some of the key themes that are emerging are
		  that membranes are patchy, with segregated regions of
		  structure and function, that lipid regions vary in
		  thickness and composition, and that crowding and
		  ectodomains limit exposure of lipid to the adjacent aqueous
		  regions.},
