@article{smith2003epl,
	abstract = {We develop a theoretical model to describe the vertical
		  pulling of vesicles adhered in a contact potential. For a
		  range of applied forces, locally stable bound shapes,
		  separated from the free shape by an energy barrier, can be
		  found. The phase diagram contains regions with either a
