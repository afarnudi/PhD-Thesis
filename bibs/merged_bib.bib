@article{tsubotajcp2014,
	abstract = {With regard to the out-of-plane bending energy of an
		  elastic surface membrane, capsule deformation was compared
		  between Helfrich's isotropic continuum model and discrete
		  models using a triangulated surface. The in-plane
		  deformation of the membrane was modeled by the neo-Hookean
		  or Skalak law. Two mechanical problems were numerically
		  simulated. One was the deformation of a spherical capsule
		  under simple shear flow and the other was the equilibrium
		  shape of a capsule with a nonzero excess surface area,
		  assuming the shape mechanics of a normal red blood cell.
		  The numerical simulations demonstrated that a discrete
		  model based on locally averaged mean curvature
		  satisfactorily reproduces the mechanical behavior
		  determined by Helfrich's isotropic continuum model, whereas
		  a discrete model without the averaging exhibited different
		  deformation behavior. Differences in the mechanical
		  behavior between bending models may cause differences in
		  estimated bending rigidity values of up to one order of
		  magnitude.},
	author = {Tsubota, Ken-ichi},
	date = {2014/11/15/},
	date-added = {2022-12-28 13:16:03 +0100},
	date-modified = {2022-12-28 13:16:18 +0100},
	doi = {https://doi.org/10.1016/j.jcp.2014.08.007},
	isbn = {0021-9991},
	journal = {Journal of Computational Physics},
	keywords = {Biophysics; Soft matter; Elastic membrane; Numerical simulation; Boundary element method},
