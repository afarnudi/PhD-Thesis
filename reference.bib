	doi = {10.1038/nrm2330},
	id = {van Meer2008},
	isbn = {1471-0080},
	journal = {Nature Reviews Molecular Cell Biology},
	number = {2},
	pages = {112--124},
	title = {Membrane lipids: where they are and how they behave},
	url = {https://doi.org/10.1038/nrm2330},
	volume = {9},
	year = {2008},
	bdsk-url-1 = {https://doi.org/10.1038/nrm2330}}

