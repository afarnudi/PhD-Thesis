	issn = {0014-4827},
	journal = {Experimental Cell Research},
	keywords = {Cell mechanics, Tissue mechanics, Mechanotransduction, Cell migration, Hill-curve, Acto-myosin interaction},
	note = {Special Issue: Cell Motility and Mechanics},
	number = {16},
	pages = {2418 - 2423},
	title = {Cell and tissue mechanics in cell migration},
	url = {http://www.sciencedirect.com/science/article/pii/S0014482713001948},
