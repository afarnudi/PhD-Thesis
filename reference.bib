	journal = {Cell Adhesion \& Migration},
	note = {PMID: 26176587},
	number = {5},
	pages = {340-344},
	publisher = {Taylor & Francis},
	title = {Epithelia migration: A spatiotemporal interplay between contraction and adhesion},
