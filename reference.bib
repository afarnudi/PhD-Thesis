	Eprint = {https://doi.org/10.1021/acs.jpcb.7b11491},
	Journal = {The Journal of Physical Chemistry B},
	Note = {PMID: 29589937},
	Number = {16},
	Pages = {4537-4545},
	Title = {Rheology of Membrane-Attached Minimal Actin Cortices},
	Url = {https://doi.org/10.1021/acs.jpcb.7b11491},
	Volume = {122},
	Year = {2018},
	Bdsk-Url-1 = {https://doi.org/10.1021/acs.jpcb.7b11491}}

