	journal = {The European Physical Journal E},
	month = {Sep},
	number = {1},
	pages = {47--53},
	title = {Force distributions and force chains in random stiff fiber networks},
	url = {https://doi.org/10.1140/epje/i2007-10209-1},
	volume = {24},
